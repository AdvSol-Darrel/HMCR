{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Hmcr": "Server=(localdb)\\mssqllocaldb;Database=aspnet-hmcr;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://*:8080"
      }
    }
  },
  "Constants": {
    "Version": "0.0.0.1",
    "SwaggerApiUrl": "/swagger/v1/swagger.json"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "File",
              "Args": {
                "path": "E:\\hmcr\\Logs\\log.txt",
                "rollingInterval": "Day",
                "retainedFileCountLimit": 30,
                "buffered": true
              }
            }
          ]
        }
      }
    ]
  },
  "ChrisUris": {
    "OasUri": "https://prdoas2.apps.th.gov.bc.ca/ogs-geoV06/wfs?",
    "MapsUri": "https://prd-maps.th.gov.bc.ca/geoV05/wfs?"
  },
  "ServiceAccount": {
    "UserId": "<user>",
    "Password": "<pass>"
  }
}
